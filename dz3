// Задание 1
package main

import (
	"fmt"
)
func main() {
	var str string
	slices := []string{""}
	fmt.Scanln(&str)
	slices = append(slices, str)
	for _, value := range slices {
		fmt.Println(value)

	}
}

// Задание 3
package main

import "fmt"

func main() {
	dict := map[string]string{
		"apple": "яблоко",
		"car":   "авто",
		"world": "мир",
	}
	for key, value := range dict {
		fmt.Printf("Key: %s, Value: %s\n", key, value)
	}
}
